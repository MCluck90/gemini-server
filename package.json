{
  "name": "gemini-server",
  "version": "1.0.0",
  "description": "My personal Gemini server",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "NODE_OPTIONS='--inspect-brk' ts-node -r tsconfig-paths/register src/main.ts",
    "lint": "eslint --ext .js,.ts src types",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=17.1.0",
    "npm": ">=8.1.2"
  },
  "dependencies": {
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "tsconfig-paths": "^3.12.0"
  }
}
